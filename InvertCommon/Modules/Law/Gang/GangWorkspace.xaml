<Page x:Class="Invert911.Gang.GangWorkspace"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    
    xmlns:GA="clr-namespace:Invert911.Gang"
    Height="440" Width="500" Loaded="UserControl_Loaded" ShowsNavigationUI="True">
    <Grid>
        <TabControl  Name="GangTabControl" Margin="0,0,0,7" TabStripPlacement="Top">
            <TabItem  Header="Main">
                <DockPanel LastChildFill="True">
                    <GA:GangMenu x:Name="MainGangMenu" Width="Auto" Height="Auto"></GA:GangMenu>
                </DockPanel>
            </TabItem>
        </TabControl>
    </Grid>
</Page>
