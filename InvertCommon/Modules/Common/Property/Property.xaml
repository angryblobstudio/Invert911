<UserControl x:Class="Invert911.InvertCommon.Modules.Property.Property"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:prop="clr-namespace:Invert911.InvertCommon.Modules.Property"         
    >
	<DockPanel >
		<TabControl>
			<TabItem Header="Details">
                <prop:PropertyDynControl x:Name="MainPropertyDynControl"></prop:PropertyDynControl>
            </TabItem>
			<TabItem Header="Notes" Visibility="Collapsed"></TabItem>
            <TabItem Header="Attachments" IsEnabled="False"></TabItem>
		</TabControl>
	</DockPanel>
</UserControl>
