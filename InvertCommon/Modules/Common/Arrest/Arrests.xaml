<UserControl x:Class="Invert911.InvertCommon.Modules.Arrest.Arrests"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:a="clr-namespace:Invert911.InvertCommon.Modules.Arrest"
    Height="300" Width="300">
    <DockPanel LastChildFill="True"  >
        <ListView DockPanel.Dock="Top" Height="119">
            <ListView.View>
                <GridView>
                    <GridViewColumn Width="140" Header="Type" />
                    <GridViewColumn Width="140" Header="Value" />
                    <GridViewColumn Width="140" Header="Description" />
                </GridView>
            </ListView.View>
        </ListView>
        <a:Arrest Width="Auto" Height="Auto"></a:Arrest>
    </DockPanel>
</UserControl>
